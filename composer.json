{
    "name": "seymos/custom_mvc",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "seymos",
            "email": "hapinow@mailo.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Config\\": "config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "monolog/monolog": "^2.0@dev",
        "symfony/http-foundation": "^5.2@dev",
        "symfony/routing": "^5.2@dev",
        "symfony/http-kernel": "^5.2@dev",
        "phpunit/phpunit": "^9.4@dev",
        "symfony/orm-pack": "dev-master",
        "symfony/event-dispatcher": "^5.2@dev",
        "symfony/security-bundle": "^5.2@dev",
        "symfony/validator": "^5.2@dev",
        "symfony/serializer": "^5.2@dev",
        "symfony/http-client": "^5.2@dev",
        "symfony/config": "^5.2@dev",
        "symfony/framework-bundle": "^5.2@dev",
        "symfony/yaml": "^5.2@dev",
        "symfony/finder": "^5.2@dev",
        "sensio/framework-extra-bundle": "^5.6@dev"
    },
    "require-dev": {
        "symfony/profiler-pack": "dev-master",
        "symfony/browser-kit": "^5.2@dev",
        "symfony/css-selector": "^5.2@dev",
        "symfony/phpunit-bridge": "^5.2@dev",
        "symfony/dotenv": "5.2.x-dev"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
